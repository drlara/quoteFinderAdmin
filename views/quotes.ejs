<body>
    
    <h1> Update/Delete Quotes</h1>

    <% for (let i of quotes) { %> 

        <a href="/updateQuote?quoteId=<%=i.quoteId%>" class="btn btn-success"> Update </a>
        Delete
         <%=i.quote%> <br>

    <% } %>    
</body>
</html>